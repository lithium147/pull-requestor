<ruleset comparisonMethod="maven"
         xmlns="https://www.mojohaus.org/VERSIONS/RULE/2.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://www.mojohaus.org/VERSIONS/RULE/2.1.0 https://www.mojohaus.org/versions/versions-model/xsd/rule-2.1.0.xsd">
    <ignoreVersions>
        <ignoreVersion type="regex">(?i).*[-_\.]M[0-9\.-]*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*[-_\.]CR[0-9\.-]*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*[-_\.]rc[0-9\.-]*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*[-_\.]beta[0-9\.-]*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*[-_\.]alpha[0-9\.-]*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*[-_\.]nightly[0-9\.-]*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*[-_\.]jboss[0-9\.-]*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-atlassian-[0-9\.-]*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-jenkins-[0-9\.-]*</ignoreVersion>
        <ignoreVersion type="regex">.*[-\.]redhat-[0-9]+</ignoreVersion>
        <ignoreVersion type="regex">(?i).*[-_\.]r[0-9]+</ignoreVersion> <!-- maven-release-plugin:3.0-r1585899 -->
        <ignoreVersion type="regex">(?i).*[-_\.0-9]beta-rev[0-9\.-]*</ignoreVersion> <!-- google-api-services-iam:v2beta-rev20220713-2.0.0 -->
        <ignoreVersion type="regex">(?i)[0-9]+\.[0-9]+\.[0-9]+-b[0-9]+\.[0-9]+</ignoreVersion> <!-- jaxb-api:2.4.0-b180830.0359 -->
        <ignoreVersion type="regex">(?i).*-SNSAPSHOT*</ignoreVersion> <!-- druid-spring-boot-starter:1.2.13-SNSAPSHOT -->
        <ignoreVersion type="regex">(?i).*\.redhat_.*</ignoreVersion> <!-- jasypt:1.9.3.redhat_3 -->
        <ignoreVersion type="regex">(?i).*-does-not-exist</ignoreVersion> <!-- commons-logging:99.0-does-not-exist -->

    </ignoreVersions>
    <rules>
        <!-- vault not compatible with protobuf 3.23+ yet -->
        <rule groupId="com.google.protobuf" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">3\.23\.\d+.*</ignoreVersion>
                <ignoreVersion type="regex">3\.24\.\d+.*</ignoreVersion>
            </ignoreVersions>
        </rule>
        <!-- jenkins is using an old version of groovy -->
        <rule groupId="org.codehaus.groovy" artifactId="groovy-all" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">3\.\d+\.\d+.*</ignoreVersion>
                <ignoreVersion type="regex">2\.5\.\d+.*</ignoreVersion>
            </ignoreVersions>
        </rule>
        <!-- ignore groovy-cps:3791.va_c0338ea_b_59c -->
        <rule groupId="com.cloudbees" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">\d{4,}.*</ignoreVersion>
            </ignoreVersions>
        </rule>
        <!-- jenkins is using groovy 2.4 -->
        <rule groupId="org.spockframework" artifactId="spock-core" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">.*-groovy-2.5</ignoreVersion>
                <ignoreVersion type="regex">.*-groovy-3.0</ignoreVersion>
                <ignoreVersion type="regex">.*-groovy-4.0</ignoreVersion>
                <ignoreVersion type="regex">.*-groovy-5.0</ignoreVersion>
            </ignoreVersions>
        </rule>
        <!-- not sure what 1.0-PFD-1 means, so just ignore it -->
        <rule groupId="javax.inject" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">1.0-PFD-1</ignoreVersion>
            </ignoreVersions>
        </rule>
        <!-- ignore 2.0.1.MR -->
        <rule groupId="jakarta.inject" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">.*\.MR</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.jenkins-ci.plugins.workflow" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">.*v.*</ignoreVersion>
                <!-- workflow-step-api:639.v6eca_cd8c04a_a_ -->
            </ignoreVersions>
        </rule>
        <rule groupId="com.google.apis" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">v1b4-rev.*</ignoreVersion>
                <!-- google-api-services-dataflow:v1b4-rev7-1.20.0 -->
            </ignoreVersions>
        </rule>
        <rule groupId="commons-*" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">\d{4,}.*</ignoreVersion>
                <!-- z.B. commons-collections:commons-collections ... 3.2.1.redhat-7 -> 20040117.000000 -->
            </ignoreVersions>
        </rule>
        <rule groupId="org.slf4j" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">.*-xwiki</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.apache.maven.plugins" artifactId="maven-surefire-plugin" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion>9c6abc2</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.codehaus.mojo" artifactId="properties-maven-plugin" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">.*-tc</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="javax.portlet" artifactId="portlet-api" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">.*-r\d+</ignoreVersion>
                <ignoreVersion type="regex">.*-Draft\d+</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
</ruleset>